<title>Dashboard - inGallery</title>
<%- layout('layout/userBoilerplate') %>

<div class="bodyContainer">
  <h1>User Dashbaord</h1>
  <h1><%= user.username %></h1>
  <h4><%= user.email %></h4>
  <h1><%= currentPage %></h1>
</div>

<p>============================================================</p>

<% if (imgs.length) { %> <% for (let img of imgs) { %>
<p><%= img.url %></p>
<% } %> <% } %>

<p>============================================================</p>

<% for (let imgDetails of publishedImages){ %>
<p><%= imgDetails.category %></p>
<% } %>
